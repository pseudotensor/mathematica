Cell[BoxData[
    \(WHICHWAVE[evn_, evalthis_] := 
      If[Abs[evalthis] < 10^\((\(-10\))\), 
        If[Abs[\(({0, 0, 0, 0, 0, kx, ky, kz} . evn)\) //. whichconst] < 
            10^\((\(-10\))\), "\<EntropyMode\>", "\<MonopoleMode\>"], 
        If[Abs[{0, 1, 0, 0, 0, 0, 0, 0} . evn\ ] < 10^\((\(-10\))\), 
          If[evalthis < 0, "\<LeftAlfvenMode\>", "\<RightAlfvenMode\>"], 
          If[Max[eval] \[Equal] evalthis, "\<RightFastMode\>", 
            If[Min[eval] \[Equal] evalthis, "\<LeftFastMode\>", 
              If[evalthis < 
                  0, "\<LeftSlowMode\>", "\<RightSlowMode\>"]]]]]\)], "Input"],

