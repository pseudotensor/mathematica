}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
    \(FullSimplify[
      gamma - mygamma, {Bsq > 0, Qtsq > 0, D > 0, Wp > 0}]\)], "Input"],

Cell[BoxData[
    \($Aborted\)], "Output"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[{
    \(god1 = 
      mygamma //. {Bsq \[Rule] 0, S \[Rule] 0}\), "\[IndentingNewLine]", 
